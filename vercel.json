{
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "cd client && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...path]"
    }
  ],
  "functions": {
    "client/api/**/*.ts": {
      "runtime": "@vercel/node@3.0.11"
    }
  }
}